{
  "manifest_version": 2,
  "name": "Whatsapp Background Changer",
  "version": "1",
  "browser_action": {
    "default_title": "Whatsapp Background Changer",
    "default_popup": "html/popup.html"
  },
  "content_scripts": [
    {
      "matches": [
        "https://web.whatsapp.com/",
        "http://web.whatsapp.com/"
      ],
      "js": ["js/content.js"]
    }
  ],
  "permissions": [
    "storage"
  ]
}